class Solution {
    public int numSubarrayProductLessThanK(int[] a, int k) {
    if(k<=1){
			return 0;
		}
		
		int product = 1;
		int ans = 0;
		int start = 0;
		for (int i = 0; i < a.length; i++) {
			product = product * a[i];
			while (product >= k) {
				product = product / a[start++];
		
			}
			ans += i - start + 1;
			// System.out.print(product + " ");
		}

		return ans;
    }
}
